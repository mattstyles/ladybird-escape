{
  "name": "ladybird-escape",
  "version": "0.0.0",
  "main": "lib",
  "scripts": {
    "lint": "standard",
    "start": "DEBUG=true NODE_PATH=src budo src/index.js --live -- --extension jsx"
  },
  "babel": {
    "presets": [
      "urban"
    ],
    "plugins": [
      "transform-runtime",
      "transform-react-require",
      "lodash",
      "styled-jsx/babel"
    ]
  },
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ],
    "extension": [
      ".js",
      ".jsx"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-react-require": "^1.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-urban": "^1.0.0",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "budo": "^9.4.7",
    "envify": "^4.1.0",
    "standard": "^10.0.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "eventemitter3": "^2.0.3",
    "lodash": "^4.17.19",
    "most": "^1.6.1",
    "raid": "^2.7.0",
    "raid-addons": "^2.8.0",
    "raid-streams": "^2.8.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "reselect": "^2.5.4",
    "styled-jsx": "^0.5.7"
  }
}
